    "test": "jest tests/",
    "test:accessibility": "axe-cli src/index.html",
    "lint": "eslint src/ --fix",
    "format": "prettier --write src/",
    "optimize": "node scripts/optimize-assets.js",
    "minify": "terser src/app.js -o src/app.min.js",
    "validate": "html5validator src/index.html"
  },
  "keywords": [
    "vibe-coding",
