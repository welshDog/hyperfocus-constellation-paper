  "main": "src/index.html",
  "homepage": "https://lyndz-williams.github.io/hyperfocus-constellation-paper",
  "scripts": {
    "start": "python -m http.server 8000",
    "dev": "live-server src/ --port=8080",
    "build": "npm run optimize && npm run minify",
